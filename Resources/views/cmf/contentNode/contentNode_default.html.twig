{% block cmf_contentNode_widget %}
    <div
        class="{{ node_class }} {{ node.classes }}"
        {% for key,value in node.attributes %}
            {{ key }}="{{ value }}"
        {% endfor %}
        >

        {#Real Content#}

        {% block cmf_contentNode_widget_content %}
            <h2>{{ node.name }}</h2>
            {% for child in node.nodes %}
                {{ child|cmfParse }}<br>
            {% endfor %}
        {% endblock %}

    </div>
{% endblock cmf_contentNode_widget %}
